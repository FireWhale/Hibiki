if( $.trim( $('#<%= @album.id %>').html() ).length ) { 
  $('#<%= @album.id %>').empty();

  $('#<%= @album.id %>').parent().children().first().css("display", "inline");
  $('#<%= @album.id %>').parent().children().first().next().css("display", "none");
  
} else {
  $('#<%= @album.id %>').html('<%= escape_javascript(render :partial => "album_preview") %>');
  
  $('#<%= @album.id %>').parent().children().first().css("display", "none");
  $('#<%= @album.id %>').parent().children().first().next().css("display", "inline");
  
  <%= render :partial => 'scripts/layouts/lazyload' %>
  
};

