history.pushState({turbolinks: true, url: '<%= request.original_url %>'},'','<%= request.original_url %>')

$('#index').html('<%= escape_javascript(render :partial => 'layouts/primary_models/record_list', locals: {records: @records}) %>');

if($('#collection-tabs li.<%= @type %>').hasClass('active') == false) {
  $('#collection-tabs li').removeClass('active');
  $('#collection-tabs li.<%= @type %>').addClass('active');
};